<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.MediaItemsEntity" table="MediaItems" schema="dbo" catalog="moshayof">
        <id name="mid">
            <column name="MID" sql-type="numeric(18)" precision="18"/>
        </id>
        <property name="prodYear">
            <column name="PROD_YEAR" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>